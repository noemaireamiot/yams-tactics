meta {
  name: Get Books
  type: graphql
  seq: 2
}

post {
  url: http://localhost:3000/graphql
  body: graphql
  auth: none
}

body:graphql {
  query {
    books {
      edges {
        node {
          id
          title
          author {
            id
            name
          }
          releaseDate
      	}
      }
    }
  }
}
